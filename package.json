{
  "name": "anecdote-engine",
  "version": "0.0.0-developent",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "license": "Apache-2.0",
  "bin": {
    "anecdote": "./lib/anecdote.js",
    "anecdote-run": "./lib/anecdote-run.js",
    "anecdote-setup": "./lib/anecdote-setup.js",
    "anecdote-add-author": "./lib/anecdote-add-author.js",
    "anecdote-scan": "./lib/anecdote-scan.js"
  },
  "scripts": {
    "start": "node ./node_modules/ts-node/dist/bin.js src/Command/anecdote",
    "setup": "node ./node_modules/ts-node/dist/bin.js src/Command/anecdote-setup",
    "add-author": "node ./node_modules/ts-node/dist/bin.js src/Command/anecdote-add-author",
    "scan": "node ./node_modules/ts-node/dist/bin.js src/Command/anecdote-scan",
    "work": "node ./node_modules/ts-node/dist/bin.js src/Command/anecdote-work",
    "generate-protobuf-meta": "node ./node_modules/protobufjs/bin/pbjs -t json-module -w es6 -o src/Protobuf.ts data/*",
    "lint": "node ./node_modules/tslint/bin/tslint src/**/*.ts",
    "pbjs": "node ./node_modules/protobufjs/bin/pbjs",
    "pbts": "node ./node_modules/protobufjs/bin/pbts",
    "tsc": "node ./node_modules/typescript/bin/tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atrauzzi/anecdote-engine.git"
  },
  "bugs": {
    "url": "https://github.com/atrauzzi/anecdote-engine/issues"
  },
  "homepage": "https://github.com/atrauzzi/anecdote-engine#readme",
  "devDependencies": {
    "@types/amqplib": "^0.5.1",
    "@types/chance": "^0.7.31",
    "@types/commander": "^2.3.31",
    "@types/dotenv": "^2.0.20",
    "@types/github": "^0.0.0",
    "@types/lodash": "^4.14.50",
    "@types/mongodb": "^2.1.37",
    "@types/node": "^7.0.4",
    "@types/node-fetch": "^1.6.7",
    "semantic-release": "^6.3.2",
    "ts-node": "^2.0.0",
    "tslint": "^4.4.1",
    "typescript": "^2.0.0"
  },
  "dependencies": {
    "amqplib": "^0.5.1",
    "chance": "^1.0.4",
    "commander": "^2.9.0",
    "dotenv": "^4.0.0",
    "github": "^9.0.0",
    "inversify": "^3.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "mongodb": "^2.2.22",
    "node-fetch": "^1.6.3",
    "protobufjs": "^6.6.2",
    "reflect-metadata": "^0.1.9"
  }
}
